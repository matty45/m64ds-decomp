	<row id="msg_0541_T11R0802_00012" index="12">
		<attribute name="window_context_name">used</attribute>
		<language name="English">This Poké Doll looks just like the\nreal thing...\rAw! It moved!\nIt’s a real BANETTE!</language>
	</row>
