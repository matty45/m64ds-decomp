	<row id="msg_0312_00000" index="0">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Welcome!\nThis is the reception for admission.\rWill you try the Pokéathlon? {YESNO 0}</language>
	</row>
	<row id="msg_0312_00001" index="1">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Welcome. This is the reception for the\nSupreme Cup.\rTop Pokéathletes are waiting for you.\nWould you like to try\fthe challenge? {YESNO 0}</language>
	</row>
	<row id="msg_0312_00002" index="2">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Anything else?</language>
	</row>
	<row id="msg_0312_00003" index="3">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Which course would you like\nto try? {YESNO 0}</language>
	</row>
	<row id="msg_0312_00004" index="4">
		<attribute name="window_context_name">used</attribute>
		<language name="English">You want to try\n{STRVAR_4 1, 3, 0}? {YESNO 0}</language>
	</row>
	<row id="msg_0312_00005" index="5">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Please come this way.\r</language>
	</row>
	<row id="msg_0312_00006" index="6">
		<attribute name="window_context_name">used</attribute>
		<language name="English">The Pokéathlon will begin right now.\nYour team color is red.\rGood luck!\r</language>
	</row>
