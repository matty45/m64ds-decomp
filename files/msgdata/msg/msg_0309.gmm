	<row id="msg_0309_00044" index="44">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Aim for number one!\nIt starts today!\r</language>
	</row>
	<row id="msg_0309_00045" index="45">
		<attribute name="window_context_name">used</attribute>
		<language name="English">First Place, {STRVAR_1 55, 1, 0} times!\nAll right! Will you come with me?\r</language>
