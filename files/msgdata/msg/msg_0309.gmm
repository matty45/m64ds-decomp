	<row id="msg_0309_00042" index="42">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Last Place, {STRVAR_1 55, 1, 0} times!\nThat doesn’t matter at all!\r</language>
	</row>
	<row id="msg_0309_00043" index="43">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Last Place, {STRVAR_1 55, 1, 0} times!\nNobody’s seen my back yet!\r</language>
