	<row id="msg_0309_00048" index="48">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Don’t worry about failure!\nFalling down is not the end!\r</language>
	</row>
	<row id="msg_0309_00049" index="49">
		<attribute name="window_context_name">used</attribute>
