	<row id="msg_0309_00060" index="60">
		<attribute name="window_context_name">used</attribute>
		<language name="English">It doesn’t end when you fall! As long\nas there’s a championship!\r</language>
	</row>
	<row id="msg_0309_00061" index="61">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Failed {STRVAR_1 55, 1, 0} times! Only the number\nof times you’ve fainted has increased!\r</language>
